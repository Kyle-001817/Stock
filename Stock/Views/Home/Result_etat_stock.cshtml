@{
    var etat = ViewData["etat"] as List<Etat_stock>;
}
<div class="content-wrapper">
    <div class="row">
        <div class="col-lg-12 mt-5">
            <div class="card">
                <div class="card-body">
                    <center><h1 style="font-family: 'Segoe UI', Tahoma, Geneva, Verdana; color:red">@ViewBag.mess</h1></center>
                <h5 class="card-title">Etat de stock</h5>
                <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                        <th scope="col">Date du debut</th>
                        <th scope="col">Date fin</th>
                        <th scope="col">Article</th>
                        <th scope="col">Quantite restante</th>
                        <th scope="col">Prix Unitaire</th>
                        <th scope="col">Magasin</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach(var item in etat){
                        <td>@item.date_debut</td>
                        <td>@item.date_fin</td>
                        <td>@item.article_id</td>
                        <td>@item.quantite_restante</td>
                        <td>@item.prix_unitaire</td>
                        <td>@item.magasin_id</td>
                        }
                    </tbody>
                    </table>
                </div>
                </div>
            </div>
        </div>
    </div>
 </div>